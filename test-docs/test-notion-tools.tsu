// test-docs/test-notion-tools.ts
import { NotionMcpConnector } from '../src/modules/workflows/connectors/mcp/notion-mcp'

async function main() {
  const connector = new NotionMcpConnector()
  await connector.connect()
  const tools = await connector.getAvailableTools()

  // 型を明示すればTS7006を防げる
  console.log('📦 MCPツール一覧:', tools.map((t: any) => t.name))
}

main().catch(err => {
  console.error('❌ 実行中にエラー:', err)
})
