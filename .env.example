# Discord
DISCORD_TOKEN=your_discord_token_here

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# MCP設定 (Claude Codeと統合するための設定)
MCP_CONFIG_PATH=/absolute/path/to/your/mcp-config.json

# Google Calendar MCP (APIモードで使用する場合のみ)
GOOGLE_CALENDAR_MCP_URL=http://localhost:3000
GOOGLE_CALENDAR_MCP_API_KEY=your_api_key_here

# Notion MCP設定
NOTION_MCP_URL=http://localhost:3002
NOTION_TASK_DB_ID=your_task_db_id
NOTION_CATEGORY_DB_ID=your_category_db_id
NOTION_STAFF_DB_ID=your_staff_db_id

# リマインダー設定
REMINDER_CHANNEL_ID=your_discord_channel_id

# Supabase
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_key_here

# Logging
LOG_LEVEL=info

# Application
NODE_ENV=development
